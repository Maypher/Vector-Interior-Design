{
  "name": "vector-portfolio",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@glidejs/glide": "^3.7.1",
    "@payloadcms/db-postgres": "3.44.0",
    "@payloadcms/db-sqlite": "3.44.0",
    "@payloadcms/live-preview-react": "3.44.0",
    "@payloadcms/next": "3.44.0",
    "@payloadcms/payload-cloud": "3.44.0",
    "@payloadcms/richtext-lexical": "3.44.0",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "next": "15.3.5",
    "next-intl": "^4.1.0",
    "payload": "3.44.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@formatjs/intl-localematcher": "^0.6.1",
    "@tailwindcss/postcss": "^4.1.6",
    "@types/glidejs__glide": "^3.6.6",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "negotiator": "^1.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.1.6",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
